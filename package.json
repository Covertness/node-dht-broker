{
  "name": "dht-peer-crawler",
  "version": "2.0.1",
  "description": "A fast and stable DHT crawler.",
  "main": "lib/crawler.mjs",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Covertness/node-dht-peer-crawler.git"
  },
  "keywords": [
    "dht",
    "crawler"
  ],
  "author": "covertness",
  "license": "MIT",
  "engines": {
    "node": ">=8.11.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "setupFiles": [
      "./jest.init.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "dependencies": {
    "bencode": "^2.0.0",
    "k-bucket": "^5.0.0",
    "quick-lru": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "coffee-script": ">= 1.10",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1"
  }
}
